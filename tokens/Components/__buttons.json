          "$type": "border",
          "$value": {
            "color": "{button.danger.active}",
            "width": "{button.border.width.sm}",
            "style": "solid"
          }
        },
        "hover": {
          "$type": "border",
          "$value": {
            "color": "{button.danger.hover}",
            "width": "{button.border.width.sm}",
            "style": "solid"
          }
        }
      }
    },
    "text": {
      "white": {
        "$type": "color",
        "$value": "{white}"
      }
    },
    "success": {
      "active": {
        "$type": "color",
        "$value": "{success.S300}"
      },
      "hover": {
        "$type": "color",
        "$value": "{success.S400}"
      },
      "hover-alt": {
        "$type": "color",
        "$value": "{success.S50}"
      },
      "click": {
        "$type": "color",
        "$value": "{success.S75}"

{
  "button": {
    "primary": {
      "active": {
        "$type": "color",
        "$value": "{primary.P300}"
      },
      "hover": {
        "$type": "color",
        "$value": "{primary.P400}"
      },
      "hover-alt": {
        "$type": "color",
        "$value": "{accent.A50}"
      },
      "click": {
        "$type": "color",
        "$value": "{accent.A75}"
      }
    },
    "secondary": {
      "active": {
        "$type": "color",
        "$value": "{secondary.S300}"
      },
      "hover": {
        "$type": "color",
        "$value": "{secondary.S400}"
      },
      "hover-alt": {
        "$type": "color",
        "$value": "{secondary.S50}"
      },
      "click": {
        "$type": "color",
        "$value": "{secondary.S75}"
      }
    },
    "disabled": {
      "$type": "color",
      "$value": "{neutral.N50}"
    },
    "borderRadius": {
      "sm": {
        "$type": "borderRadius",
        "$value": "{borderRadius.4}"
      },
      "xxxl": {
        "$type": "borderRadius",
        "$value": "{borderRadius.24}"
      }
    },
    "border": {
      "width": {
        "sm": {
          "$type": "borderWidth",
          "$value": "{borderWidth.01}"
        }
      },
      "primary": {
        "active": {
          "$type": "border",
          "$value": {
            "color": "{button.primary.active}",
            "width": "{button.border.width.sm}",
            "style": "solid"
          }
        },
        "hover": {
          "$type": "border",
          "$value": {
            "color": "{button.primary.hover}",
            "width": "{button.border.width.sm}",
            "style": "solid"
          }
        }
      },
      "secondary": {
        "active": {
          "$type": "border",
          "$value": {
            "color": "{button.secondary.active}",
            "width": "{button.border.width.sm}",
            "style": "solid"
          }
        },
        "hover": {
          "$type": "border",
          "$value": {
            "color": "{button.secondary.hover}",
            "width": "{button.border.width.sm}",
            "style": "solid"
          }
        }
      },
      "success": {
        "active": {
          "$type": "border",
          "$value": {
            "color": "{button.success.active}",
            "width": "{button.border.width.sm}",
            "style": "solid"
          }
        },
        "hover": {
          "$type": "border",
          "$value": {
            "color": "{button.success.hover}",
            "width": "{button.border.width.sm}",
            "style": "solid"
          }
        }
      },
      "danger": {

        "$type": "color",
        "$value": "{danger.D300}"
      },
      "hover": {
        "$type": "color",
        "$value": "{danger.D400}"
      },
      "hover-alt": {
        "$type": "color",
        "$value": "{danger.D50}"
      },
      "click": {
        "$type": "color",
        "$value": "{danger.D75}"
      }
    },
    "info": {
      "active": {
        "$type": "color",
        "$value": "{Info.I300}"
      },
      "hover": {
        "$type": "color",
        "$value": "{Info.I400}"
      },
      "hover-alt": {
